package day01;

// 실무에서는 공동작업이 중요하기 때문에
// Class를 하나의 파일에 모든 것을 만들어 두지 않음

public class ClassTest {
	public static void main(String[] args) {
		Student kim = new Student();	// 객체 생성
		Student park = new Student();	// 객체 생성
		
		kim.name = "김철수";
		kim.kor = 90;
		kim.eng = 70;
		
		park.name = "박영희";
		park.kor = 100;
		park.eng = 80;
	}
}
